    "next": "15.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/form-data": "^2.2.1",    
    "@types/lodash": "^4.17.18",
    "@types/node": "^20",
